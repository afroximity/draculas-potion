    "tokenColors": [
        {
            "scope": [
                "emphasis"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "strong"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "header"
            ],
            "settings": {
                "foreground": "#BD93F9"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "unused.comment",
                "wildcard.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6272A4"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#F1FA8C"
            }
        },
        {
            "name": "String quotes",
            "scope": [
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end"
            ],
            "settings": {
                "foreground": "#E9F284"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#BD93F9"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#BD93F9"
            }
        },
        {
            "name": "Boolean constants",
            "scope": [
                "constant.language.boolean",
                "constant.language"
            ],
            "settings": {
                "foreground": "#BD93F9"
            }
        },
        {
            "name": "Escape sequences",
            "scope": [
                "constant.character.escape",
                "constant.character.string.escape",
                "constant.regexp"
            ],
            "settings": {
                "foreground": "#FF79C6"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "punctuation.definition.keyword"
            ],
            "settings": {
                "foreground": "#FF79C6"
            }
        },
        {
            "name": "Keyword new",
            "scope": [
                "keyword.control.new",
                "keyword.operator.new"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Storage types",
            "scope": [
                "storage",
                "storage.type",
